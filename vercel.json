{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/link/(.*)",
      "dest": "/api/link.js"
    },
    {
      "src": "/api/deferred-deeplink",
      "dest": "/api/deferred-deeplink.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.js"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    }
  ],
  "env": {
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}